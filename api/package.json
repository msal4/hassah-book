{
  "dependencies": {
    "apollo-server-express": "^2.16.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^59.0.0",
    "graphql": "^15.3.0",
    "graphql-query-complexity": "^0.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0-rc.3",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mock-req-res": "^1.1.2",
    "@types/node": "^14.0.26",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "mock-req-res": "^1.2.0",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "api:run": "ts-node -r tsconfig-paths/register",
    "api:run-dev": "ts-node-dev -r tsconfig-paths/register",
    "dev": "yarn api:run-dev src/index.ts",
    "test:setup": "yarn api:run src/test-utils/setup.ts",
    "test": "yarn test:setup && jest -i",
    "test:watch": "yarn test --watch",
    "seed:config": "yarn api:run ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "yarn api:run ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "yarn api:run ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "yarn api:run ./node_modules/typeorm/cli.js schema:sync",
    "drop-and-seed": "yarn schema:drop && yarn schema:sync && yarn seed:run"
  }
}
